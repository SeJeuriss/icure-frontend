{
  "html" : "<html>\n   <head>\n      <style>@page {size: landscape; margin: 0; padding: 0; }body {margin: 0; padding: 0; font-family: /* \"Open Sans\", */ Arial, Helvetica, sans-serif; line-height:1.3em; }.page { color:#000000; font-size:12px; position:relative; /* border:1px solid #f00; */ }.present{padding : 5px;height: 100%;margin-top: 0;box-sizing: border-box;}table{    border-collapse: collapse;}.top{    border-top : 1px solid black;}.bot{    border-bottom : 1px solid black;}.right{    border-right : 1px solid black;}.left{    border-left : 1px solid black;}\n      .small-top{    border-top : 1px solid #d0d0d0;}\n      .high-top{    border-top : 2px solid black;}\n      .vertical{ font-size: 20px;}\n      .small-right{    border-right : 1px solid #d0d0d0;}.small-left{    border-left : 1px solid #d0d0d0;}.small-bot{    border-bottom : 1px solid #d0d0d0;}.poso{    white-space: nowrap;    display : block;    width: 100px;    overflow: hidden;    text-overflow: ellipsis;}.center{    text-align: center;}</style>\n   </head>\n   <body>\n      <div class=\"page\">\n         <div class=\"present\">\n            <table>\n               <tr>\n                  <td class=\"top left\" colspan=\"3\"/>\n                  <th class=\"top right\" colspan=\"5\">\n                     <span>{{localize.mom_break}}</span>\n                  </th>\n                  <th class=\"top left right\" colspan=\"4\">\n                     <span>{{localize.mom_lunch}}</span>\n                  </th>\n                  <th class=\"top left\" colspan=\"4\">\n                     <span>{{localize.mom_dinner}}</span>\n                  </th>\n                  <td class=\"top right\"></td>\n               </tr>\n               <tr>\n                  <td class=\"bot left top\" style=\"width: 20px;\"/>\n                  <td class=\"bot top small-right\" style=\"max-width: 100px;\">{{localize.medic}}</td>\n                  <td class=\"bot top small-right small-left\">{{localize.freq}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\"><img alt=\"\" src=\"data:image/x-png;base64,{{sun}}\"/></td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_before}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_during}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_after}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">10h</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_before}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_during}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_after}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">16h</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_before}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_during}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\">{{localize.mom_after}}</td>\n                  <td class=\"bot top small-right small-left\" style=\"width: 30px;\"><img alt=\"\" src=\"data:image/x-png;base64,{{moon}}\"/></td>\n                  <td class=\"bot right top small-left\">{{localize.endDate}}</td>\n               </tr>\n               {{#list}}\n               <tr>\n                  <td class=\"high-top left\"></td>\n                  <td class=\"high-top right\" colspan=\"16\">\n                     <img alt=\"\" src=\"data:image/x-png;base64,{{history}}\" width=\"16px\" height=\"16px\"/>\n                     <span>\n                        <b>{{localizedTitle}}</b>\n                     </span>\n                  </td>\n               </tr>\n              {{#meds}}\n              <tr>\n                  <td class=\"small-top left\">\n                    <img alt=\"\" src=\"data:image/x-png;base64,{{euro}}\" width=\"16px\" height=\"16px\"/>\n                  </td>\n                  <td class=\"small-top small-right\">{{serviceDescription}}</td>\n                  <td class=\"small-top small-right poso\">{{servicePosology}}</td>\n                  {{#combinedTableHeaders0}}\n                     <td class=\"small-top small-right center\">{{administratedQuantity.quantity}}</td>\n                  {{/combinedTableHeaders0}}\n                  {{^combinedTableHeaders0}}\n                     <td class=\"small-top small-right center\"></td>\n                  {{/combinedTableHeaders0}}\n                  <td class=\"small-top small-right center\">{{regimenTable.1.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.2.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.3.0.administratedQuantity.quantity}}</td>\n                  {{#combinedTableHeaders4}}\n                     <td class=\"small-top small-right center\">{{administratedQuantity.quantity}}</td>\n                  {{/combinedTableHeaders4}}\n                  {{^combinedTableHeaders4}}\n                     <td class=\"small-top small-right center\"></td>\n                  {{/combinedTableHeaders4}}\n                  <td class=\"small-top small-right center\">{{regimenTable.5.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.6.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.7.0.administratedQuantity.quantity}}</td>\n                  {{#combinedTableHeaders8}}\n                     <td class=\"small-top small-right center\">{{administratedQuantity.quantity}}</td>\n                  {{/combinedTableHeaders8}}\n                  {{^combinedTableHeaders8}}\n                     <td class=\"small-top small-right center\"></td>\n                  {{/combinedTableHeaders8}}\n                  <td class=\"small-top small-right center\">{{regimenTable.9.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.10.0.administratedQuantity.quantity}}</td>\n                  <td class=\"small-top small-right center\">{{regimenTable.11.0.administratedQuantity.quantity}}</td>\n                  {{#combinedTableHeaders12}}\n                     <td class=\"small-top small-right center\">{{administratedQuantity.quantity}}</td>\n                  {{/combinedTableHeaders12}}\n                  {{^combinedTableHeaders12}}\n                     <td class=\"small-top small-right center\"></td>\n                  {{/combinedTableHeaders12}}\n                  <td class=\"small-top right center\">\n                     <span>{{getMedicationClosingDate}}</span>\n                  </td>\n               </tr>\n               {{#medication.instructionForPatient}}\n               <tr>\n                  <td></td>\n                  <td class=\"right left small-top bot\" colspan=\"16\">\n                     <span>{{localize.com}} : {{medication.instructionForPatient}}</span>\n                  </td>\n               </tr>\n               {{/medication.instructionForPatient}}\n               {{^medication.instructionForPatient}}\n               <tr>\n                  <td colspan=\"17\" class=\"bot left right\"/>\n               </tr>\n               {{/medication.instructionForPatient}}\n               {{/meds}}\n               {{/list}}\n\n            </table>\n         </div>\n      </div>\n   </body>\n</html>",
